{
  "rewrites": [
    { "source": "/coming-soon", "destination": "/coming-soon.html" },
    { "source": "/contact", "destination": "/contact.html" }
  ],
  "headers": [
    {
      "source": "/:all*(css|js|png|jpg|jpeg|gif|svg|mp4|webm|ico|woff|woff2|ttf|eot)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
